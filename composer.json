{
    "name": "horrentotaal/hamstra-api",
    "version": "1.2.2",
    "description": "Horrentotaal hamstra API",
    "private": false,
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://horrentotaal.nl",
    "license": "proprietary",
    "authors": [
        {
            "name": "Horrentotaal contributers",
            "homepage": "https://github.com/horrentotaal/hamstra-api"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/",
        "access": "public"
    }
}
